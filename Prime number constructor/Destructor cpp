#include<iostream> 
using namespace std; 
class prime { 
  int a,k,i; 
  public: prime(int x) {   //constructor
    a=x; 
  } 
  public: ~prime() {       //destructor
  }
  void calculate() { 
    k=1; { 
      for(i=2;i<=a/2;i++) 
        if(a%i==0) { 
          k=0; 
          break; 
        } 
        else { 
          k=1; 
        } 
    } 
  } 
  void show() { 
    if(k==1) 
      cout<< "\nThe Number is prime Number.\n";  
    else 
      cout<<"\nThe Number is Not prime.\n"; 
  } 
};
int main() { 
  int a; 
  cout<<"\nEnter any Number: "; 
  cin>>a; 
  prime p(a); 
  p.calculate(); 
  p.show(); 
  return 0; 
}


